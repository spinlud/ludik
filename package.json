{
  "name": "ludik",
  "version": "1.0.0",
  "description": "LUDIK experimental website. Main technologies: node.js, vue.js, webgl, three.js, webaudio, webpack.",
  "engines": {
    "node": "11.x"
  },
  "main": "server.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --https -p --config webpack.prod.js",
    "start": "node server.js",
    "certbot-ssl-certificate": "./certbot/generate-certificate.sh",
    "certbot-ssl-certificate-renew": "./certbot/renew-certificate.sh",
    "docker-stop-containers": "docker ps -a | awk -F '[ ]+' 'NR>1 {print($1)}' | xargs -n1 docker stop | xargs -n1 docker rm --volumes",
    "docker-delete-images": "docker rmi $(docker images -q)",
    "docker-compose": "docker compose up -d --build",
    "docker-compose-logs": "docker compose logs -f -t"
  },
  "author": "Ludovico Fabbri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinlud/ludik.git"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.3.0",
    "body-parser": "^1.18.3",
    "config": "^3.1.0",
    "cookie-parser": "^1.4.4",
    "dat.gui": "^0.7.5",
    "express": "^4.16.4",
    "express-http-to-https": "^1.1.4",
    "http-status-codes": "^1.3.0",
    "jquery": "^3.4.1",
    "morgan": "^1.9.1",
    "opentype.js": "^0.11.0",
    "path": "^0.12.7",
    "screenfull": "^4.2.0",
    "smoothscroll-polyfill": "^0.4.3",
    "stats-js": "^1.0.0",
    "stereo-panner-node": "^1.4.0",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "favicons-webpack-plugin": "6.0.0-alpha.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "imports-loader": "^0.8.0",
    "node-sass": "^4.12.0",
    "resolve-url-loader": "^3.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "vue-loader": "^15.6.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "keywords": [
    "music",
    "webaudio",
    "webgl",
    "three",
    "three.js",
    "threejs",
    "node.js",
    "nodejs",
    "vue.js",
    "webpack"
  ]
}
